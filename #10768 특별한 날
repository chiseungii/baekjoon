// C++

#include <iostream>
using namespace std;

int main() {
	int month, day; cin >> month >> day;

	if (month < 2) cout << "Before\n";
	else if (month > 2) cout << "After\n";
	else {
		if (day < 18) cout << "Before\n";
		else if (day > 18) cout << "After\n";
		else cout << "Special\n";
	}
}




// Python

month = int(input())
day = int(input())

if month < 2:
    print("Before")
elif month > 2:
    print("After")
else:
    if day < 18:
        print("Before")
    elif day > 18:
        print("After")
    else:
        print("Special")
