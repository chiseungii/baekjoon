// C++

#include <iostream>
using namespace std;

int main() {
	int sum_min = 0;
	for (int i = 0; i < 4; i++) {
		int min; cin >> min;
		sum_min += min;
	}
	cout << sum_min / 60 << endl;
	cout << sum_min % 60 << endl;
}




// Python

sum_min = 0
for i in range(4):
    min = int(input())
    sum_min += min
print(sum_min // 60)
print(sum_min % 60)
