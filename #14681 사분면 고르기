// C++

#include <iostream>
using namespace std;

int main() {
	int x, y; cin >> x >> y;

	if (x > 0) {
		if (y > 0) cout << 1 << endl;
		else cout << 4 << endl;
	}
	else {
		if (y > 0) cout << 2 << endl;
		else cout << 3 << endl;
	}
}




// Python

x = int(input())
y = int(input())

if x > 0:
    if y > 0:
        print(1)
    else:
        print(4)
else:
    if y > 0:
        print(2)
    else:
        print(3)
