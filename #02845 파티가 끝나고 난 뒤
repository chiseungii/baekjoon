// C++

#include <iostream>
using namespace std;

int main() {
	int L, P; cin >> L >> P;
	int people = L * P;

	for (int i = 0; i < 5; i++) {
		int n; cin >> n;
		cout << n - people << ' ';
	}
	cout << endl;
}




// Python

L, P = map(int, input().split())
people = L * P
nums = [int(n) for n in input().split()]

for i in range(5):
    print(nums[i] - people, end=' ')
