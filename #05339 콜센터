// C++

#include <iostream>
using namespace std;

int main() {
	cout << "     /~\\\n";
	cout << "    ( oo|\n";
	cout << "    _\\=/_\n";
	cout << "   /  _  \\\n";
	cout << "  //|/.\\|\\\\\n";
	cout << " ||  \\ /  ||\n";
	cout << "============\n";
	cout << "|          |\n";
	cout << "|          |\n";
	cout << "|          |\n";
}




// Python

print("     /~\\")
print("    ( oo|")
print("    _\\=/_")
print("   /  _  \\")
print("  //|/.\\|\\\\")
print(" ||  \\ /  ||")
print("============")
print("|          |")
print("|          |")
print("|          |")
