// C++

#include <iostream>
using namespace std;

int main() {
	cout << "       _.-;;-._\n";
	cout << "'-..-'|   ||   |\n";
	cout << "'-..-'|_.-;;-._|\n";
	cout << "'-..-'|   ||   |\n";
	cout << "'-..-'|_.-''-._|\n";
}




// Python

print("       _.-;;-._")
print("'-..-'|   ||   |")
print("'-..-'|_.-;;-._|")
print("'-..-'|   ||   |")
print("'-..-'|_.-''-._|")
