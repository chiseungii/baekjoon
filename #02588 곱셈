// C++

#include <iostream>
using namespace std;

int main() {
	int n1, n2; cin >> n1 >> n2;

	int copy_n = n2;
	for (int i = 0; i < 3; i++) {
		cout << n1 * (copy_n % 10) << endl;
		copy_n /= 10;
	}
	cout << n1 * n2 << endl;
}




// Python

n1 = int(input())
n2 = int(input())

copy_n = n2
for i in range(3):
    print(n1 * (copy_n % 10))
    copy_n //= 10
print(n1 * n2)
