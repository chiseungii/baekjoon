// C++

#include <iostream>
using namespace std;

int main() {
	int n; cin >> n;
	bool leap_year = false;

	if (n % 4 == 0)
		if (n % 100 != 0 || n % 400 == 0)
			leap_year = true;

	cout << leap_year << endl;
}




// Python

n = int(input())
leap_year = 0

if n%4 == 0:
    if (n%100 != 0) | (n%400 == 0):
        leap_year = 1
        
print(leap_year)
