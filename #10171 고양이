 // C++
 #include <iostream>
 using namespace std;
 
 int main(){
 	cout << "\\    /\\\n";
 	cout << " )  ( ')\n";
 	cout << "(  /  )\n";
 	cout << " \\(__)|\n";
 }
 
 
 
 
 // Java
 public class Main {
	public static void main(String[] args) {
		System.out.println("\\    /\\");
		System.out.println(" )  ( ')");
		System.out.println("(  /  )");
		System.out.println(" \\(__)|");
	}	
}
