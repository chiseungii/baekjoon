// C++

#include <iostream>
using namespace std;

int main() {
	cout << "|\\_/|\n"
		<< "|q p|   /}\n"
		<< "( 0 )\"\"\"\\\n"
		<< "|\"^\"`    |\n"
		<< "||_/=\\\\__|\n";
}




// Python

print("|\\_/|",
      "|q p|   /}",
      '( 0 )"""\\',
      '|"^"`    |',
      "||_/=\\\\__|",
      sep='\n')
